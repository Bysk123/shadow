<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <remove name="php" />
            <add name="php" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="D:\xampp\php\php.exe" resourceType="File" requireAccess="Script" />
        </handlers>
    </system.webServer>
</configuration>
